

{% extends "paceeGameBundle::layout.html.twig" %}

{% block title %}{{ parent() }} {{game.name}}{% endblock %}


{% block body %}
    
    
                        <div class="col-sm-push-2 col-sm-8">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                {{game.name}}
                            </div>
                            <div class="panel-body">
                                <div>
                                 <iframe src="{{game.uri}}/index.html" width="{{game.width}}" height="{{game.height}}" frameborder="0" style="display:block" allowfullscreen></iframe>
                                </div>
                            </div>
                            <div class="panel-footer">
                                <div class="form-group">
                                    <label for="focusedInput">link</label>
                                    <input class="form-control" id="focusedInput" type="text" value="{{url('pacee_Game_page',{'name':game.uri})}}">
                                </div>
                          {{game.infos}}
                            </div>
                        </div>
                    </div
{% endblock %}  